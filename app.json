{
  "expo": {
    "name": "Audio Recorder",
    "slug": "audio-recorder",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.audiorecorder"
    },
    "android": {
      "package": "com.yourname.audiorecorder",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash.png"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "39b59454-3f4f-4982-bab5-990f8d8fc70a"
      }
    }
  }
}